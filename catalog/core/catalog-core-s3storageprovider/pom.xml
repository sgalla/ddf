            <artifactId>netty-common</artifactId>
            <version>4.1.17.Final</version>
        </dependency>
        <dependency>
            <groupId>io.netty</groupId>
            <artifactId>netty-transport</artifactId>
            <version>4.1.17.Final</version>
        </dependency>
        <dependency>
            <groupId>io.netty</groupId>
            <artifactId>netty-resolver</artifactId>
            <version>4.1.17.Final</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <configuration>
                    <instructions>
                        <Bundle-SymbolicName>
                            ${project.artifactId};blueprint.graceperiod:=true;blueprint.timeout:=604800000
                        </Bundle-SymbolicName>
                        <Embed-Dependency>
                            catalog-core-api-impl,
                            platform-util,
                            aws-java-sdk-osgi,
                            ion-java,
                            commons-logging,
                            commons-codec,
                            joda-time,
                            httpclient-osgi,
                            httpcore-osgi,
                            jackson-dataformat-cbor,
                            jackson-annotations,
                            jackson-databind,
                            jackson-core,
                            netty-codec-http,
                            netty-codec,
                            netty-handler,
                            netty-buffer,
                            netty-common,
                            netty-transport,
                            netty-resolver
                        </Embed-Dependency>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

</project>
